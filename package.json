{
  "name": "n8n-nodes-npm-watch",
  "version": "0.1.8",
  "description": "Watch npm dist-tags (defalut latest) and emit items on new versions for use with Slack/Email/etc.",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "npm",
    "watch"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Khy",
    "email": "khy@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imHay0ung/n8n-nodes-npm-watch"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest"
  },
  "files": [
    "dist",
    "index.js",
    "README.md",
    "LICENSE.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/NpmWatch/NpmWatch.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}